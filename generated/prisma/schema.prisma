// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = "file:./company.db"
}

model Workers {
  id            Int      @id @default(autoincrement())
  nome          String
  cargo         String
  salario       Float
  data_admissao DateTime
}
